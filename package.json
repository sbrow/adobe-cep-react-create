    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rewire": "^4.0.1",
    "ts-loader": "^5.3.3",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.4.1",
    "winston": "^3.2.1"
  }
