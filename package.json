{
  "name": "workout-builder",
  "version": "0.4.2",
  "author": "Spencer Brower",
  "contributors": [
    "Tomer Riko Shalev"
  ],
  "description": "demo for CEP",
  "license": "ISC",
  "main": "./client/src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config ./src/client/webpack.config.js --env.target=web --open --mode development",
    "build": "node ./build-scripts/build.js",
    "build:dev": "npm run build",
    "build:prod": "npm run build --production",
    "build:jsx": "tsc -p ./src/host/tsconfig.json",
    "deploy": "node ./build-scripts/deploy.js",
    "deploy:dev": "npm run deploy",
    "deploy:prod": "npm run deploy --production",
    "archive": "node ./build-scripts/archive.js",
    "release": "npm run build && npm run deploy",
    "release:dev": "npm run release",
    "release:prod": "npm run release --production && npm run archive",
    "babel": "babel ./client/js --out-dir ./client/out",
    "test": "ts-mocha -r jsdom-global/register -p ./src/client/tsconfig.json ./src/client/test/test.spec.ts",
    "make": "ts-node ./build-scripts/make.ts",
    "watch:client": "webpack --config ./src/client/webpack.config.js --mode development --watch --display minimal",
    "watch:session": "webpack --config ./src/session/webpack.config.js --mode development --watch --display minimal",
    "watch": "concurrently \"npm run build:jsx -- -w\" \"npm run watch:client\" \"npm run watch:session\"",
    "clean": "rm -f ./logs/*",
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" --exclude \"./src/**/*.d.ts\"",
    "prebuild": "npm run clean",
    "preversion": "git stash save \"pre-version\"",
    "version": "npm version",
    "postversion": "git stash pop"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/events": "^3.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.4",
    "@types/rewire": "^2.5.28",
    "@types/winston": "^2.4.4",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "json": "^9.0.6",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^6.1.3",
    "rewire": "^4.0.1",
    "style-loader": "^0.21.0",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "zxp-sign-cmd": "^1.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.2",
    "@material-ui/icons": "^2.0.0",
    "console-feed": "^2.8.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "winston": "^3.2.1"
  }
}