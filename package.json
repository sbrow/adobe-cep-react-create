{
  "name": "workout-builder",
  "version": "0.9.0",
  "author": "Spencer Brower",
  "contributors": [
    "Tomer Riko Shalev"
  ],
  "description": "demo for CEP",
  "license": "ISC",
  "main": "./client/src/index.tsx",
  "scripts": {
    "start:dev": "webpack-dev-server --config ./src/client/webpack.config.js --env.target=web --open --mode development",
    "test": "ts-mocha -r jsdom-global/register -p ./src/client/tsconfig.json ./src/client/test/test.spec.ts",
    "build": "node ./build-scripts/build.js",
    "build:dev": "npm run build",
    "build:prod": "npm run build --production",
    "build:jsx": "tsc -p ./src/host/tsconfig.json",
    "deploy": "sudo node ./build-scripts/deploy.js",
    "deploy:dev": "npm run deploy",
    "deploy:prod": "npm run deploy --production",
    "archive": "node ./build-scripts/archive.js",
    "release": "npm run build:prod && npm run archive",
    "make": "ts-node ./build-scripts/make.ts",
    "watch:client": "webpack-cli --config ./src/client/webpack.config.js --mode development --watch --display minimal",
    "watch:session": "webpack-cli --config ./src/session/webpack.config.js --mode development --watch --display minimal",
    "watch": "concurrently \"npm run build:jsx -- -w\" \"npm run watch:client\" \"npm run watch:session\"",
    "clean": "rm -f ./logs/* && rm -rf ./dist",
    "check": "depcheck ./",
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" --exclude \"./src/**/*.d.ts\"",
    "dump:types": "ts-node ./build-scripts/dumpTypes.ts",
    "prebuild": "npm run lint && npm run clean",
    "preversion": "git stash save \"pre-version\"",
    "postversion": "git stash pop",
    "prepare": "npm make --production && npm check && npm lint && npm build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/events": "^3.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/rewire": "^2.5.28",
    "@types/shelljs": "^0.8.5",
    "@types/winston": "^2.4.4",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "depcheck": "^0.7.2",
    "extendscript-xml-to-typescript": "github:pravdomil/extendscript-xml-to-typescript",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "json": "^9.0.6",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rewire": "^4.0.1",
    "shelljs": "^0.8.3",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.2.1",
    "zxp-provider": "^1.2.0",
    "zxp-sign-cmd": "^1.0.0"
  },
  "dependencies": {}
}